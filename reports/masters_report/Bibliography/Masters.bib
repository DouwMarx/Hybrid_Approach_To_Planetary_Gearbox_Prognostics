keywords = {Empirical mode decomposition,Fault diagnosis,Feature evaluation,Radial basis function network,Wavelet packet transform},
number = {6},
pages = {9941--9948},
publisher = {Elsevier Ltd},
title = {{Application of an intelligent classification method to mechanical fault diagnosis}},
url = {http://dx.doi.org/10.1016/j.eswa.2009.01.065},
volume = {36},
