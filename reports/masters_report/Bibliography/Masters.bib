@article{Chang2002,
abstract = {One unique feature of neural networks is that they have to be trained to function. In developing an iterative neural network technique for model updating of structures, it has been shown that the number of training samples required increases exponentially as the number of parameters to be updated increases. Training the neural network using these samples becomes a time-consuming task. In this study, we investigate the use of orthogonal arrays for the sample selection. A comparison between this orthogonal arrays method and four other methods is illustrated by two numerical examples. One is the update of the felxural rigidities of a simply supported beam and the other is the update of the material properties and the boundary conditions of a circular plate. The results indicate that the orthogonal arrays method can significantly reduce the number of training samples without affecting too much the accuracy of the neural network prediction. {\textcopyright} 2002 Elsevier Science Ltd. All rights reserved.},
author = {Chang, C. C. and Chang, T. Y.P. and Xu, Y. G. and To, W. M.},
doi = {10.1006/jsvi.2001.3915},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Artificial intelligence and machine learning/2002 Chang et al Selection of training samples for model updating using neurla networks.pdf:pdf},
isbn = {0022-460X},
issn = {0022460X},
journal = {Journal of Sound and Vibration},
number = {5},
pages = {867--883},
title = {{Selection of training samples for model updating using neural networks}},
volume = {249},
year = {2002}
}
@article{Perez-Ortiz2003,
abstract = {The long short-term memory (LSTM) network trained by gradient descent solves difficult problems which traditional recurrent neural networks in general cannot. We have recently observed that the decoupled extended Kalman filter training algorithm allows for even better performance, reducing significantly the number of training steps when compared to the original gradient descent training algorithm. In this paper we present a set of experiments which are unsolvable by classical recurrent networks but which are solved elegantly and robustly and quickly by LSTM combined with Kalman filters. {\textcopyright} 2003 Elsevier Science Ltd. All rights reserved.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {P{\'{e}}rez-Ortiz, Juan Antonio and Gers, Felix A. and Eck, Douglas and Schmidhuber, J{\"{u}}rgen U.},
