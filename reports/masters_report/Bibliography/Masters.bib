publisher = {Elsevier B.V.},
title = {{Bayesian Networks-based Association Rules and Knowledge Reuse in Maintenance Decision-Making of Industrial Product-Service Systems}},
url = {http://dx.doi.org/10.1016/j.procir.2016.03.046},
