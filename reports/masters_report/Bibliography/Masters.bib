keywords = {Digital Pulse Processing,Digital Spectroscopy,FPGA,Frontend Electronics,Programmable Logic,Reconfigurable Instrumentation,Silicon Drift Detectors,Single Photon Detection,System-On-Chip,X-Ray Fluorescence},
pages = {520--525},
