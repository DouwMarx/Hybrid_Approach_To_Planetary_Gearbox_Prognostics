journal = {Mechanical Systems and Signal Processing},
keywords = {Fault condition recognition,Multiple target hypothesis,Sequential hypothesis testing,Sequential probability ratio test (SPRT)},
number = {2},
pages = {469--482},
publisher = {Elsevier},
title = {{Multiple fault condition recognition of gearbox with sequential hypothesis test}},
url = {http://dx.doi.org/10.1016/j.ymssp.2013.06.023},
volume = {40},
year = {2013}
}
@article{Feki2013,
abstract = {This paper deals with the simulation of the dynamic behavior of induction motor/gear systems. The corresponding model combines: (i) a permeance network aimed at representing squirrel-cage induction motors and (ii) a 3D dynamic model of spur and helical gears including the shafts and bearings. Some classic motor defects (rotor eccentricity) and gear faults (tooth spalls) are considered and an iterative solution technique is presented which makes it possible to solve the coupled electro-mechanical problem. Based on a number of simulations, it is shown that the frequencies associated with defects can be identified and that distant detection and monitoring of tooth faults using stator currents seem promising alternatives to the classic techniques based on mechanical vibration analysis. {\textcopyright} 2012 Elsevier B.V. All rights reserved.},
