file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Condition based maintenance and condition assessment/2015 Ma Review of dynamics of cracked gear systems.pdf:pdf},
issn = {13506307},
journal = {Engineering Failure Analysis},
keywords = {Analytical method,Crack propagation path,Cracked gear systems,Finite element method,Time-varying mesh stiffness (TVMS)},
pages = {224--245},
publisher = {Elsevier Ltd},
title = {{Review on dynamics of cracked gear systems}},
url = {http://dx.doi.org/10.1016/j.engfailanal.2015.06.004},
volume = {55},
year = {2015}
