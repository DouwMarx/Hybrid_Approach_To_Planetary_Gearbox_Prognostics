@article{Abboud2016a,
abstract = {In rotating machine diagnosis, the squared envelope spectrum (SES) is one of the most efficient indicators of the presence of cyclostationarity, which is typical symptom of damages in rolling element bearing. Similarly to other cyclostationary tools, the SES should be applied on the pure random part of the signal otherwise spurious peaks may appear and hide the cyclostationary content. Therefore, conventional deterministic$\backslash$random separation (DRS) techniques are usually used as a pre-processing step to suppress the deterministic (periodic) component. In speed varying regimes, the SES is combined with computed order tracking to obtain an order-domain representation rather than a Hertzian one. Though being order tracked, the deterministic components undergo speed-dependent variations in their magnitude and phase, thus losing their periodicity. This necessarily invalidates the working assumption of conventional DRS techniques and, consequently, jeopardizes the efficiency of the SES. Lately, some sophisticated preprocessing techniques have been proposed to deal with this issue whereof the most important are the improved synchronous average (ISA), the cepstrum prewhitening (CPW) and the generalized synchronous average (GSA). This paper aims to provide a comparative study of these techniques by highlighting their pros and cons. The comparison is performed on real-word signals.},
author = {Abboud, D and Eltabach, M and Antoni, J and Sieg-Zieba, S},
doi = {10.1784/204764216819375258},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Bearings and rotors/2015 Antoni et al Envelope pre-processing techniques for rolling element bearing diagnosis in variable speed conditions.pdf:pdf},
issn = {20476426},
journal = {International Journal of Condition Monitoring},
number = {2},
pages = {27--32},
pmid = {11381037},
title = {{Envelope pre-processing techniques for rolling element bearing diagnosis in variable speed conditions}},
url = {http://openurl.ingenta.com/content/xref?genre=article{\&}issn=2047-6426{\&}volume=6{\&}issue=2{\&}spage=27},
volume = {6},
year = {2016}
