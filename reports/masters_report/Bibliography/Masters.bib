pmid = {23194186},
publisher = {Elsevier},
title = {{Condition based maintenance optimization for multi-component systems using proportional hazards model}},
url = {http://dx.doi.org/10.1016/j.ress.2010.12.023},
volume = {96},
year = {2011}
}
