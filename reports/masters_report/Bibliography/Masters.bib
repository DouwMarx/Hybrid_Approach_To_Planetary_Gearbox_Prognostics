@article{Thompson2006,
abstract = {The maintenance management of unpaved mine road networks varies widely,$\backslash$nfrom small low tonnage operations where ad hoc techniques are applied,$\backslash$nto extensive networks of roads subject to large numbers of high axle$\backslash$nloadings, where haul road maintenance management systems (MMS) have$\backslash$nbeen successfully implemented. However, for complex road networks$\backslash$nwhere material is sourced and hauled from a large and highly variable$\backslash$nnumber of loading points, the MMS becomes onerous. Since most large$\backslash$nmines operate ultra heavy mine trucks with on board diagnostic data$\backslash$ncollation, relaying data within a broader asset management and centralised$\backslash$ncommunication and GPS backbone, it was recognised that road condition$\backslash$ncould be monitored on a realtime basis and the data integrated within$\backslash$nthe centralised truck management system. This paper summarises the$\backslash$nresults of a number of field trials undertaken to prove the concept$\backslash$nof realtime road maintenance management, using truck derived multisensor$\backslash$ndata capture. The paper concludes that truck on board multisensor$\backslash$ndata integration with the existing mine communication and asset management$\backslash$nsystems can provide the basis for a realtime mine road MMS, with$\backslash$nresultant improvement in service and reduction in total road user$\backslash$ncosts. {\textcopyright} 2006 Institute of Materials, Minerals and Mining.},
author = {Thompson, R. J. and Visser, A. T. and Heyns, P. S. and Hugo, D.},
doi = {10.1179/174328606X155147},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2006 Thompson Visser Heyns Hugo Mine road maintenance management.pdf:pdf},
issn = {1474-9009},
journal = {Mining Technology},
keywords = {haul roads,multibody dynamics modelling,road maintenance,surface mining},
number = {4},
pages = {123--128},
title = {{Mine road maintenance management using haul truck response measurements}},
url = {http://www.tandfonline.com/doi/full/10.1179/174328606X155147},
volume = {115},
year = {2006}
}
@article{VanTonder2006,
abstract = {Dynamic loading may excite higher-order dynamic modes that participate in the stress response of a structure. If this is the case, techniques taking these dynamic effects into account have to be considered in approximating the stress response of the structure. We show that the equivalent static design criterion can be extended to account for the dynamic modes participating in the response of a structure, and that the extension is equivalent to the mode-acceleration superposition method. This allows the formulation of an efficient and economically viable dynamic fatigue assessment method that is novel, since no time or frequency domain fatigue analysis is required. Finite element analysis and experimental strain gauge measurement are integral to the method, which we verify numerically by considering a cantilevered beam subjected to dynamic excitation.},
author = {{Van Tonder}, F. and Heyns, P. S. and Wannenburg, J.},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2006 Van Tonder Heyns Wannenburg Dynamic response in the fatigue analysis of a structure.pdf:pdf},
issn = {00382353},
journal = {South African Journal of Science},
number = {11-12},
pages = {527--532},
title = {{Dynamic response in the fatigue analysis of a structure due to unknown narrowband stochastic loading}},
volume = {102},
year = {2006}
}
@article{Oberholster2006,
abstract = {This paper presents a methodology for monitoring the on-line condition of axial-flow fan blades with the use of neural networks. In developing this methodology, the first stage was to utilise neural networks trained on features extracted from on-line blade vibration signals measured on an experimental test structure. Results from a stationary experimental modal analysis of the structure were used for identifying global blade mode shapes and their corresponding frequencies. These in turn were used to assist in identifying vibration-related features suitable for neural network training. The features were extracted from on-line blade vibration and strain signals which were measured using a number of sensors. The second stage in the development of the methodology entails utilising neural networks trained on numerical Frequency Response Function (FRF) features obtained from a Finite Element Model (FEM) of the test structure. Frequency domain features obtained from on-line experimental measurements were used to normalise the numerical FRF features prior to neural network training. Following training, the networks were tested using experimental frequency domain features. This approach makes it unnecessary to damage the structure in order to train the neural networks. The paper shows that it is possible to classify damage for several fan blades by using neural networks with on-line vibration measurements from sensors not necessarily installed on the damaged blades themselves. The significance of this is that it proves the possibility to perform on-line fan blade damage classification using less than one sensor per blade. Even more significant is the demonstration that an on-line damage detection system for a fan can be developed without having to damage the actual structure. {\textcopyright} 2004 Elsevier Ltd. All rights reserved.},
author = {Oberholster, A. J. and Heyns, P. S.},
doi = {10.1016/j.ymssp.2004.09.007},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2006 Oberholster {\_} Heyns On-line Fan Blade Damage Detection.pdf:pdf},
issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
number = {1},
pages = {78--93},
title = {{On-line fan blade damage detection using neural networks}},
volume = {20},
year = {2006}
}
@article{Stander2006,
abstract = {Vibration can be monitored under fluctuating load conditions if provision is made for taking into account the fluctuation in machine speed, the response amplitude modulation caused by the change in input force, and the amplitude and phase effects on the measured response from the transmission path. Methodologies have been developed to compensate for the effects of fluctuating speed and amplitude modulation. However, this article investigates the effect of the transmission path phase. This is discussed in terms of the effect this phase has on synchronous averaging. A new approach is presented to resolve the influence that the transmission path phase has on synchronous averaging. The approach is used for the experimental data measured on a helical gear test rig. A significant improvement in the rate of convergence was obtained by adopting the new approach which compensates for the phase shifting in the measured structural response. This contrasts with conventional synchronous averaging with order tracking which does not compensate for structural response phase shifting. {\textcopyright} 2005 Elsevier Ltd. All rights reserved.},
author = {Stander, C. J. and Heyns, P. S.},
doi = {10.1016/j.ymssp.2005.05.009},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2006 Stander Heyns Transmission path phase compensation for gear monitoring.pdf:pdf},
isbn = {0888-3270},
issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Fluctuating load conditions,Helical gear,Order tracking,Synchronous averaging,Transmission path phase,Vibration},
number = {7},
pages = {1511--1522},
title = {{Transmission path phase compensation for gear monitoring under fluctuating load conditions}},
volume = {20},
year = {2006}
}
@article{Dacheux2001,
abstract = {The management of unpaved mine road networks - characterized by high$\backslash$naxle loadings, low traffic volumes, variable materials and construction$\backslash$nquality, and rapid rates of deterioration - is often inadequate.$\backslash$nThis situation results in either overmaintenance of the road or failure$\backslash$nto recognize significant deterioration, which both lead to the application$\backslash$nof suboptimal road maintenance strategies with the attendant increase$\backslash$nin total road-user costs. A real-time condition-triggered maintenance$\backslash$nmanagement system was identified as a solution, in which onboard$\backslash$nmonitoring of vehicle dynamic response to road condition forms the$\backslash$nbasis of road defect recognition and maintenance response. A practical$\backslash$napproach to road defect reconstruction using measured truck response$\backslash$nis presented. Initially, the application context is introduced, then$\backslash$nthe field testing program is described, in which data sets were generated$\backslash$nthat served as the basis for mathematical modeling of the truck response.$\backslash$nA modeling approach utilizing the concept of independent front suspension$\backslash$ndynamic equilibrium is described as the basis for road defect recognition.$\backslash$nOn the basis of measured suspension forces and the acceleration of$\backslash$nthe unsprung mass, the approach to tire force and road defect reconstruction$\backslash$nis presented. It is concluded that the methodology developed enables$\backslash$nreconstruction of road defect geometries with an accuracy sufficient$\backslash$nto allow specific types and dimensions of defects to be recognized$\backslash$nfor the purpose of road maintenance. By extending the methodology$\backslash$nto public unpaved roads, maintenance could be applied as and where$\backslash$nneeded with a resultant reduction in authority cost and improvement$\backslash$nin service provided for the road user.},
author = {Dacheux, F and Dacheux, J L},
