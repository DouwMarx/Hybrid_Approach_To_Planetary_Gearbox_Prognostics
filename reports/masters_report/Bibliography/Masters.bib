@article{Bartelmus2009b,
abstract = {The paper shows the needs to create condition degradation models. Mostly, in the models one fault and its development is taken into consideration. The gearing faults for gearboxes are thought as local and distributed. In gearboxes also occur local faults for rolling element bearings. As considering a gearbox condition monitoring mostly it is not taken the influence of varying external load. Recently, the new diagnostic feature has been developed by the authors, which is defined as the gearbox susceptibility/yielding characteristic to operation condition (load, velocity variability). In this case another fault has to be taken into consideration. It has been found the influence of increased bearing backlash to the gearbox susceptibility/yielding characteristic to the operation condition. The increased backlash fault is the result of harsh environment in which gearboxes operate. Mostly, it is influenced by very fine hard dust particles, which get into gearbox oil and cause the abrasive wear of gearbox elements. The paper will give computer simulation results, which show the influence of the increased rolling element bearing backlash to the load yielding characteristic and influence of other distributed faults. There are also given the gearbox susceptibility/yielding characteristics to the operation condition, which are obtained from real measurements.},
author = {Bartelmus, W. and Zimroz, R.},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2009 Bartelmus Zimroz Gearbox condition degradation models.pdf:pdf},
isbn = {9781618390097},
journal = {6th International Conference on Condition Monitoring and Machinery Failure Prevention Technologies 2009},
pages = {1--10},
title = {{Gearbox condition degradation models}},
volume = {1},
year = {2009}
}
@article{Amarnath2009,
abstract = {Gears are one of the most common mechanisms for transmitting power and motion and their usage can be found in numerous applications. Studies on gear teeth contacts have been considered as one of the most complicated applications in tribology. Depending on the application, the speed and load conditions of teeth may change triggering several types of failures on teeth surface such as wear, scuffing, micro-pitting and pitting. The above-mentioned faults influence changes in vibration and acoustic signals, due to changes in operating conditions such as increase in temperature and decrease in lubricant film thickness and specific film thickness. These abnormal changes result in cumulative effects on localised or distributed faults on load bearing surfaces of gears. Such damages cause reduction in tooth stiffness and severity of damage can be assessed by evaluating the same using vibration-based signals. This paper presents the results of experimental investigations carried out to assess wear in spur gears of back-to-back gearbox under accelerated test conditions. The studies considered the estimation of operating conditions such as film thickness and their effects on the fault growth on teeth surface. Modal testing experiments have been carried out on the same gear starting from healthy to worn out conditions to quantify wear damage. The results provide a good understanding of dependent roles of gearbox operating conditions and vibration parameters as measures for effective assessment of wear in spur gears. {\textcopyright} 2008.},
author = {Amarnath, M. and Sujatha, C. and Swarnamani, S.},
doi = {10.1016/j.triboint.2008.07.008},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2009 Amarnath et al Experimental studies on the effects of reduction in gear tooth stiffness and lubricant film thickness in a spur geared system.pdf:pdf},
isbn = {1521-3773; 1433-7851},
issn = {0301679X},
journal = {Tribology International},
keywords = {Gear tooth stiffness,Gear vibration,Gear wear,Lubricant film thickness},
number = {2},
pages = {340--352},
pmid = {22173813},
title = {{Experimental studies on the effects of reduction in gear tooth stiffness and lubricant film thickness in a spur geared system}},
volume = {42},
year = {2009}
}
@article{Zamanian2011,
abstract = {The features of non-stationary multi-component signals are often difficult to be extracted for expert systems. In this paper, a new method for feature extraction that is based on maximization of local Gaussian correlation function of wavelet coefficients and signal is presented. The effect of empirical mode decomposition (EMD) to decompose multi-component signals to intrinsic mode functions (IMFs), before using of local Gaussian correlation is discussed. The experimental vibration signals from two gearbox systems are used to show the efficiency of the presented method. Linear support vector machine (SVM) is utilized to classify feature sets extracted with the presented method. The obtained results show that the features extracted in this method have excellent ability to classify faults without any additional feature selection; it is also shown that EMD can improve or degrade features according to the utilized feature reduction method. {\textcopyright} 2011 Elsevier B.V. All rights reserved.},
author = {Zamanian, Amir Hosein and Ohadi, Abdolreza},
doi = {10.1016/j.asoc.2011.06.020},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2010 Gear Fault Diagnosis Based on Gaussian Correlation of sdarticle2.pdf:pdf},
issn = {15684946},
journal = {Applied Soft Computing Journal},
keywords = {EMD,Fault detection,Gaussian-correlation,Gear,SVM,Wavelet},
number = {8},
pages = {4807--4819},
publisher = {Elsevier B.V.},
title = {{Gear fault diagnosis based on Gaussian correlation of vibrations signals and wavelet coefficients}},
url = {http://dx.doi.org/10.1016/j.asoc.2011.06.020},
volume = {11},
year = {2011}
}
@article{Mark2010a,
abstract = {Fixed transducers often are used to monitor meshing gear pairs in order to detect tooth damage. A simple frequency-domain damage-detection algorithm is suggested for very early detection of such damage. Ratios of rotational-harmonic amplitudes computed from before and after potential damage are utilized to eliminate effects of transducer and structural-path-caused amplitude changes between tooth-meshing location and transducer output, to minimize attenuating effects of multiple-tooth contact, and thereby, to approximately equally weight rotational-harmonic amplitudes over a wide range of harmonics. Statistical averaging of absolute values of logarithmic ratios of rotational-harmonic amplitudes is used to minimize fluctuations caused by multiple-tooth contact and manufacturing errors on the subject gear. Synchronous averaging is employed to minimize effects of noise and manufacturing errors on the mating gear. Time-windowing tailored to contact ratios of mating gears is utilized to isolate individual tooth locations. Resultant windowing effects on availability of useful rotational harmonics are analyzed. Application of the algorithm to detection of seeded bending-fatigue faults on a planetary ring-gear tooth indicates that successful detections were achieved. {\textcopyright} 2010 Elsevier Ltd. All rights reserved.},
author = {Mark, William D. and Lee, Hyungdae and Patrick, Romano and Coker, Joseph D.},
doi = {10.1016/j.ymssp.2010.04.004},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2010 Mark et al A simple frequency domain algorithm for early detection of damaged gear teeth.pdf:pdf},
issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Diagnostics,Epicyclic gears,Gear-health monitoring,Planetary gears,Transmission error},
number = {8},
pages = {2807--2823},
publisher = {Elsevier},
title = {{A simple frequency-domain algorithm for early detection of damaged gear teeth}},
url = {http://dx.doi.org/10.1016/j.ymssp.2010.04.004},
volume = {24},
year = {2010}
}
@article{Inalpolat2009,
abstract = {In this paper, a simplified mathematical model is proposed to describe the mechanisms leading to modulation sidebands of planetary gear sets. The model includes key system parameters such as number of planets, planet position angles, and planet phasing relationships defined by the position angles and the number of teeth of the gears. The model is used to simulate a wide range of gear sets to show that they can be classified in five distinct groups based on their sideband behavior in terms of their frequencies and amplitudes. A special experimental planetary gear set-up is developed and planetary gear sets from of three of these five groups are procured. A methodology is developed to demonstrate modulation sidebands from the ring (internal) gear radial acceleration measurements. For each case, sets of ring gear acceleration measurements at various speed and torque conditions are presented to demonstrate rich sideband activity that agrees well with the model predictions. At the end, based on results of the parametric studies and experiments, general rules on modulation sidebands of planetary gear sets are proposed. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
author = {Inalpolat, M. and Kahraman, A.},
doi = {10.1016/j.jsv.2009.01.004},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2009 Inalpolat Kahraman A theoretical and experimental investigation of modulation side bands of planetary gear sets.pdf:pdf},
isbn = {1365-2982 (Electronic)$\backslash$r1350-1925 (Linking)},
issn = {0022460X},
journal = {Journal of Sound and Vibration},
number = {3-5},
pages = {677--696},
pmid = {22292889},
title = {{A theoretical and experimental investigation of modulation sidebands of planetary gear sets}},
volume = {323},
year = {2009}
}
@article{Zimroz,
abstract = {In many complex mechanical systems (drive unit with multi-stage gearbox) there is a chance to find a multi faults problem. These faults may appear in different location, they may represent different type of fault and generate different signals with different amplitudes, frequency content and statistical properties. Most of proposed in the literature techniques concern nearly "ideal" condition of all components in machine and introduced artificially one type of fault (mainly local damage) at particular location (often experiments are done on test stands). In the paper a different case will be considered as an example of real problem of condition monitoring in normal (nonstationary) operation in industrial environment. A comprehensive description of machine with multi faults problem will be shown. A scheme of signal processing for fault diagnosis will be proposed. Proposed scheme consist of: detection of multi-faults problem (Spectral Correlation Density), specific Signal of Interest extraction, fault detection and recognition and change of condition estimation and life time prognosis. Machines work under non-stationary load, consequently signals are non-stationary, too (in sense of time varying amplitudes and frequency content). Time varying (load dependent) signals/features require multidimensional analysis (features and operating condition indicators). SCD doesn't contain information about time (it is function of two frequencies). Components on SCD plane are averaged (like in classic spectrum). It usually gives a chance to detect faults, however in order to estimate fault development one needs to take into account influence of size of damage and load/speed value.},
author = {Zimroz, Radoslaw},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2009 Zimroz Some remarks on local damage diagnosis in teh presence of multi-faults and non-stationary operation.pdf:pdf},
isbn = {9781618390097},
journal = {Analysis},
pages = {33--44},
title = {{Some remarks on local damage diagnosis in presence of multi-faults and non-stationary operation}}
}
@article{Ozturk2010,
abstract = {Conventional methods (i.e. time, frequency and cepstrum) can routinely be used to reveal fault-indicating information in the vibration signal. In recent years, Wavelet analysis, which can lead to the clear identification of the nature of faults, are widely used to describe machine condition. Capability of this method in the detection of any abnormality can be further improved when its low-order frequency moments are considered. This paper presents the use of vibration-based techniques in the early detection and advancement monitoring of distributed pitting fault. The pits were seeded on all of the gear tooth surfaces in differing degrees of severity, and intended to replicate the pitting damage due to misalignment. With each fault severity, the helical gears were tested and the resulting vibration data were recorded. The application of employed vibration-based methods (i.e. time, frequency, cepstrum, and wavelet transform: scalogram and its mean frequency variation) to each set of experimental data are presented. It has been found that presence of pitting fault cannot be clearly revealed by the conventional unless fault severity is significantly large. In contrast, the scalogram and especially its mean frequency variation provide early indications of presence and progression of pitting faults in gears even when the fault severity is considerably smaller. {\textcopyright} Springer Science+Business Media, LLC 2010.},
author = {Ozturk, Hasan and Yesilyurt, Isa and Sabuncu, Mustafa},
doi = {10.1007/s10921-010-0066-4},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2010 Ozturk Detection and advancement monitoring of distibuted pitting failure in gears.pdf:pdf},
issn = {01959298},
journal = {Journal of Nondestructive Evaluation},
keywords = {Distributed gear pit,Fault detection,Gearbox,Mean frequency,Scalogram},
number = {2},
pages = {63--73},
title = {{Detection and advancement monitoring of distributed pitting failure in gears}},
volume = {29},
year = {2010}
}
@article{Wang2010,
abstract = {Varying load can cause changes in a measured gearbox vibration signal. However, conventional techniques for fault diagnosis are based on the assumption that changes in vibration signal are only caused by deterioration of the gearbox. There is a need to develop a technique to provide accurate state indicator of gearbox under fluctuating load conditions. This paper presents an approach to gear fault diagnosis based on complex Morlet continuous wavelet transform under this condition. Gear motion residual signal, which represents the departure of time synchronously averaged signal from the average tooth-meshing vibration, is analyzed as source data due to its lower sensitiveness to the alternating load condition. A fault growth parameter based on the amplitude of wavelet transform is proposed to evaluate gear fault advancement quantitatively. We found that this parameter is insensitive to varying load and can correctly indicate early gear fault. For a comparison, the advantages and disadvantages of other measures such as kurtosis, mean, variance, form factor and crest factor, both of residual signal and mean amplitude of continuous wavelet transform waveform, are also discussed. The effectiveness of the proposed fault indicator is demonstrated using a full lifetime vibration data history obtained under sinusoidal varying load. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
author = {Wang, Xiyang Y. and Makis, Viliam and Yang, Ming},
doi = {10.1016/j.jsv.2009.11.010},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2010 Wang Makis Yang A wavelet approach to fault diagnosis of a gearbox under varying load conditions.pdf:pdf},
isbn = {0022-460X},
issn = {0022460X},
journal = {Journal of Sound and Vibration},
number = {9},
pages = {1570--1585},
publisher = {Elsevier},
title = {{A wavelet approach to fault diagnosis of a gearbox under varying load conditions}},
url = {http://dx.doi.org/10.1016/j.jsv.2009.11.010},
volume = {329},
year = {2010}
}
