journal = {Fatigue and Fracture of Engineering Materials and Structures},
keywords = {Condition-based monitoring,Natural frequency,Semi-elliptical surface crack,Torsional stiffness},
number = {8},
pages = {734--747},
title = {{Torsion response of a cracked stainless steel shaft}},
volume = {30},
year = {2007}
}
