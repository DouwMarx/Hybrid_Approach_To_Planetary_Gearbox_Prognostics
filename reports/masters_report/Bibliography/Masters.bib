journal = {La reproduction chez les mammif{\`{e}}res et l'homme - Nouvelle {\'{e}}dition},
number = {1989},
pages = {290--315},
title = {{L'{\'{e}}pididyme et les glandes annexes}},
volume = {2},
year = {2001}
}
@article{Eggers2007,
abstract = {This study investigates the application of computed order tracking$\backslash$nwith subsequent rotation domain averaging and statistical analysis,$\backslash$nto the drag gearbox of a dragline used in a mining environment. Computed$\backslash$norder tracking is a fault detection method developed to deal with$\backslash$nthe varying speed conditions often found in industry and has been$\backslash$nproven effective in laboratory conditions. Its application to real$\backslash$nsituations, however, requires some adjustment to deal with issues$\backslash$nsuch as inadequate speed data and the fact that the drag gear rotates$\backslash$nin two directions. This provides a unique opportunity to observe$\backslash$nthe performance of the order tracking method in a bi-directional$\backslash$nrotating environment, allowing an investigation of the relationships$\backslash$nbetween the results obtained from each operating direction. A monitoring$\backslash$nstation was set up aboard the dragline and data was captured twice$\backslash$ndaily for a period spanning approximately one year. The data captured$\backslash$nconsisted of accelerometer and proximity sensor data. The key on$\backslash$nthe shaft triggers the proximity sensors, allowing speed and direction$\backslash$nto be measured. The measured speed is interpolated by using various$\backslash$nspeed interpolation techniques. Then the interpolated speed is used$\backslash$nto complete the order tracking procedure that resamples the vibration$\backslash$ndata with reference to the speed. The results indicate that computed$\backslash$norder tracking can be successfully employed in real environments.$\backslash$nFurthermore it is shown that the rotation direction that opens the$\backslash$ngear-tooth crack gives a better indication of incipient failure.$\backslash$nIt is therefore important not to disregard either direction when$\backslash$nmonitoring rotating machinery of this kind. {\textcopyright} The Southern African$\backslash$nInstitute of Mining and Metallurgy, 2007.},
author = {Eggers, B. L. and Heyns, P. S. and Stander, C. J.},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2007 Eggers Heyns Stander Using order tracking to detect gear condition aboard a dragline.pdf:pdf},
isbn = {0038-223X},
issn = {22256253},
journal = {Journal of the Southern African Institute of Mining and Metallurgy},
number = {2},
pages = {115--122},
title = {{Using computed order tracking to detect gear condition aboard a dragline}},
volume = {107},
year = {2007}
}
@article{Grove2007,
abstract = {This study entails the fatigue analysis of a complex plate-like structure subjected to random loading. The stress and fatigue life assessment is performed by means of experimental strain gauge measurements, finite element analysis and a quasi-static fatigue assessment procedure known as the fatigue equivalent static load (FESL) methodology. Firstly, the integrity of shell elements for accurately capturing the stiffness properties and stress distribution in the vicinity of welds is investigated. Furthermore, the stress response of the structure to random dynamic loading is investigated and validated in terms of its suitability for assessment by the FESL methodology. Nominal stress and hot-spot stress fatigue life predictions are made, based on measurements as well as the FESL procedure. The viability and integrity of the FESL methodology is critically assessed after which the actual fatigue life of the structure under particular loading conditions is determined for comparison. 2007 The Authors. Journal compilation 2007 Blackwell Publishing Ltd.},
author = {Grov{\'{e}}, A. P. and van Tonder, Francois and Heyns, P. S.},
