journal = {Chinese Journal of Aeronautics},
keywords = {Aerodynamic optimization,Dimensional reduction,Improved multi-objective particle swarm optimizati,Multi-objective,Principal component analysis},
number = {4},
pages = {1336--1348},
publisher = {Chinese Society of Aeronautics and Astronautics},
title = {{Aerodynamic multi-objective integrated optimization based on principal component analysis}},
url = {http://dx.doi.org/10.1016/j.cja.2017.05.003},
volume = {30},
year = {2017}
}
@article{Abboud2015,
abstract = {In speed-varying conditions, the assumption of cyclostationarity of rolling-element bearing vibrations is jeopardized. The emitted signal comprises an interaction between (i) time-dependent components related to the system dynamics (e.g. transfer function) and (ii) angle-dependent mechanisms related to the system kinematics (e.g. impact, load modulations...). This necessarily implies the inadequacy of classical cyclostationary tools no matter a temporal or angular vision is adopted. This consequently calls for an angle$\backslash$time approach which preserves - via the angle variable - the cyclic evolution of the signal while maintaining - via the time variable - a temporal description of the system dynamics. The first object of this paper is to analytically characterize bearing fault vibrations and explore its angle$\backslash$time cyclostationary property. The second object is to experimentally validate these results on real-world vibration signals and demonstrate the optimality of the angle-time approach over classical approaches for rolling element bearing diagnosis.},
author = {Abboud, Dany and Antoni, Jerome and Eltabach, Mario and Sieg-Zieba, Sophie},
doi = {10.1016/j.measurement.2015.07.017},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Big data/Probability and statistics/2015 Abboud AntoniEltabach Sieg-Zieba Angle-time cyclostationary for the analysis of rolling element bearing vibrations.pdf:pdf},
isbn = {0263-2241},
issn = {02632241},
journal = {Measurement: Journal of the International Measurement Confederation},
keywords = {Anglentime cyclostationarity,Bearing diagnosis,Cyclostationarity,Nonstationary conditions,Order frequency spectral correlation,Spectral correlation},
pages = {29--39},
publisher = {Elsevier Ltd},
title = {{Angle$\backslash$time cyclostationarity for the analysis of rolling element bearing vibrations}},
url = {http://dx.doi.org/10.1016/j.measurement.2015.07.017},
volume = {75},
year = {2015}
}
