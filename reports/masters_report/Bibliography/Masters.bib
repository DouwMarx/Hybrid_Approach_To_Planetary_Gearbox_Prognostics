issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Eulerian laser Doppler vibrometry,Non-harmonic Fourier analysis,Online blade condition monitoring,Phase angle trends,Tracking laser Doppler vibrometry},
