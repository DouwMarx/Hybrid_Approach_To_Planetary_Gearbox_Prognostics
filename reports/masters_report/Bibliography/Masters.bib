@article{Cococcioni2013,
abstract = {This paper presents a method, based on classification techniques, for$\backslash$nautomatic detection and diagnosis of defects of rolling element$\backslash$nbearings. The experimental data set consists of vibration signals$\backslash$nrecorded by four accelerometers on a mechanical device including rolling$\backslash$nelement bearings: the signals were collected both with all faultless$\backslash$nbearings and after substituting one faultless bearing with an$\backslash$nartificially damaged one. Four defects and, for one of them, three$\backslash$nseverity levels are considered. Classification accuracy higher than 99{\%}$\backslash$nwas achieved in all the experiments performed on the vibration signals$\backslash$nrepresented in the frequency domain, thus proving the high sensitivity$\backslash$nof our method to different types of defects and to different degrees of$\backslash$nfault severity. The degree of robustness of our method to noise is also$\backslash$nassessed by analyzing how the classification performance varies with the$\backslash$nsignal-to-noise ratio and using statistical classifiers and neural$\backslash$nnetworks.},
