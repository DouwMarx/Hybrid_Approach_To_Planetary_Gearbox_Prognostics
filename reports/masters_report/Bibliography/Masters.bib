@article{Wannenburg2008,
abstract = {A methodology is presented to derive a statistical fatigue loading$\backslash$nprofile of the total population of users of a vehicle model, from$\backslash$nfailure data recorded on the same or a previous model. The method$\backslash$nis based on fitting a bivariate probability density function on normalised$\backslash$nfailure data. This is radically more economic than existing methods.$\backslash$nReasonable accuracy could be achieved, even if the failures represent$\backslash$nonly a small fraction of the total population. The two-parameter$\backslash$nusage profile determined in this way offers a powerful approach to$\backslash$npredict failures or derive statistically based durability test or$\backslash$ndesign requirements. Copyright {\textcopyright} 2008 Inderscience Enterprises Ltd.},
