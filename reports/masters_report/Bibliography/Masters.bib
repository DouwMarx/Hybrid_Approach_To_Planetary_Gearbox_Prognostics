keywords = {CBM,CDP,Condition based maintenance,Context driven prognostic,Fingerprint,Operational data,Remaining useful life,Rul},
number = {1},
pages = {181--185},
publisher = {Elsevier B.V.},
title = {{Context driven remaining useful life estimation}},
url = {http://dx.doi.org/10.1016/j.procir.2014.07.129},
volume = {22},
year = {2014}
}
