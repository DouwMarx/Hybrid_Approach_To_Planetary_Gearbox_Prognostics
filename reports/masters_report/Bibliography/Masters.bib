@article{Abboud2018,
abstract = {The vibration-based condition monitoring of planetary gears is a highly active and challenging field of research. Many signal processing techniques have been proposed with the aim of promoting the fault component in the signal and, consequently, highlight the fault signature (i.e. the damage symptom). Most of these techniques considers the fault contribution as being deterministic, which is true in the case of an advanced and localized faults. Such techniques may fail in other scenarii in which, for instance, the fault is of distributed nature and its vibrational component is weak. In such a case, the fault component is likely to be randomized and turns cyclostationary. The present paper suggests the presence of an additional cyclostationary component in planetary gear vibrations. The presence of this component is explained by the presence of load fluctuations at the meshing points and the random micro-irregularity in the gear component stiffness, as well as the presence of distributed faults. A simplified signal model is proposed to explain the vibration signal structure of a healthy and faulty (with planet fault) planetary gear. Also, a cyclostationary-based condition monitoring approach is proposed, being based on the cyclic coherence. The proposed approach is validated on a real-world vibration signals acquired from a planetary gear benchmark.},
author = {Abboud, D and Becquerelle, M and S, Elbadaoui and Lalmi, M},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Gear vibration/2018 Abboud et al The application of the cyclic coherence for distributed plant fault detection in planetary gears.pdf:pdf},
journal = {Ingentaconnect.Com},
number = {1},
pages = {1--12},
title = {{The application of the cyclic coherence for distributed planet fault detection in planetary gears}},
url = {https://www.ingentaconnect.com/contentone/bindt/ijcm/2018/00000008/00000002/art00004},
volume = {8},
year = {2018}
}
