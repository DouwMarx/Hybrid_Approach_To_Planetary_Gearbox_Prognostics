isbn = {0660137410},
issn = {00207683},
journal = {International Journal of Solids and Structures},
keywords = {Correlogram,Forming press,High-resolution methods,Vibration ball bearing},
number = {24-25},
pages = {4293--4313},
title = {{High-resolution methods in vibratory analysis: Application to ball bearing monitoring and production machine}},
volume = {38},
year = {2001}
}
@article{Klein2011a,
