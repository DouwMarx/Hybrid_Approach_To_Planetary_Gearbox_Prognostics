issn = {0931-0509},
journal = {Proceedings of the 54th Meeting of the Society for Machinery Failure Prevention Technology, Virginia Beach, VA},
keywords = {a system,by the demand to,cbm has been driven,condition-based maintenance,diagnostic features,dynamic systems,increase,mechanical fault simulation,model-based diagnosis,modeling,of unnecessary maintenance in,prognostics,system efficiency through elimination},
pages = {203--212},
title = {{Dynamic simulation of mechanical fault transition}},
url = {http://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:DYNAMIC+SIMULATION+OF+MECHANICAL+FAULT+TRANSITION{\#}0},
