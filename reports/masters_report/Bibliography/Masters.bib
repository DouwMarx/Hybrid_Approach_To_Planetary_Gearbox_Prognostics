issn = {1542-8818},
keywords = {2nd-order optimization,conver-,gence rate,natural gradient methods,neural networks,parameterization invariance},
pmid = {23653543},
title = {{New insights and perspectives on the natural gradient method}},
url = {http://arxiv.org/abs/1412.1193},
year = {2014}
}
