issn = {09410643},
journal = {Neural Computing and Applications},
keywords = {Condition monitoring,Hidden Markov models,Neural networks,Tool wear},
number = {4},
pages = {325--336},
title = {{A comparative evaluation of neural networks and hidden Markov models for monitoring turning tool wear}},
