doi = {10.1016/j.ymssp.2006.01.006},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/Vibration monitoring/2007 Pan Liao Chiu Improvement on Gabor order tracking and objective comparison with Vold-Kalman filtering.pdf:pdf},
