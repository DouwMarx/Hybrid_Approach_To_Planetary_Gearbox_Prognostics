journal = {Journal of Sound and Vibration},
number = {3-5},
pages = {623--646},
publisher = {Elsevier},
title = {{Fault detection in variable speed machinery: Statistical parameterization}},
url = {http://dx.doi.org/10.1016/j.jsv.2009.07.025},
volume = {327},
year = {2009}
}
