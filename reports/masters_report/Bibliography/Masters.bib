journal = {Annals of Occupational Hygiene},
keywords = {Mining,Noise,Rock drills,South Africa,Vibration},
number = {3},
pages = {308--310},
pmid = {17229762},
title = {{Rock drills used in South African mines: A comparative study of noise and vibration levels}},
volume = {51},
year = {2006}
}
@article{Freyer2008,
abstract = {Besides reducing the restricting effects of tool vibrations on productivity,$\backslash$nwork-piece surface finish and tool life, it is desirable to handle$\backslash$nlack of space for sensors at the tool tip and the cost of control$\backslash$nsystems in turning processes in an effective way. This work considers$\backslash$nthese two aspects by exploiting the concept of a self-sensing actuator$\backslash$n(SSA) in the simulation of tool vibration control. The tool holder$\backslash$nstructure, in its passive as well as active state, is modeled as$\backslash$na supported cantilever. A feedback filtered-x least-mean-square (LMS)$\backslash$nalgorithm is chosen to compute the control action. A known technique,$\backslash$nwhich consists of pre-filtering the inputs to the LMS-algorithm maintains$\backslash$nthe stability of the control system. The self-sensing path is modeled$\backslash$nand illustrated. It consists of the transmission of the tool tip$\backslash$ndisplacement to the SSA where it is sensed by converting it into$\backslash$na voltage signal. A considerable reduction of 93{\%} of the displacement$\backslash$nr.m.s. values of the tool tip, was obtained when simulating this$\backslash$ncontrol system. {\textcopyright} 2008 SAGE Publications Los Angeles.},
author = {Freyer, B. H. and Theron, N. J. and Heyns, P. S.},
doi = {10.1177/1077546307085389},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2008 Freyer Theron Heyns Simulation of tool vibration control in turning using a self-sensing actuator.pdf:pdf},
isbn = {0322002210011},
issn = {10775463},
journal = {JVC/Journal of Vibration and Control},
keywords = {Active control,Self-sensing actuator,Tool vibration,Turning process},
number = {7},
pages = {999--1019},
title = {{Simulation of tool vibration control in turning, using a self-sensing actuator}},
volume = {14},
year = {2008}
}
@article{Herzog2009a,
abstract = {This paper concerns the use of neural networks for predicting the residual life of machines and components. In addition, the advantage of using condition-monitoring data to enhance the predictive capability of these neural networks was also investigated. A number of neural network variations were trained and tested with the data of two different reliability-related datasets. The first dataset represents the renewal case where the failed unit is repaired and restored to a good-as-new condition. Data were collected in the laboratory by subjecting a series of similar test pieces to fatigue loading with a hydraulic actuator. The average prediction error of the various neural networks being compared varied from 431 to 841 s on this dataset, where test pieces had a characteristic life of 8971 s. The second dataset were collected from a group of pumps used to circulate a water and magnetite solution within a plant. The data therefore originated from a repaired system affected by reliability degradation. When optimized, the multi-layer perceptron neural networks trained with the Levenberg-Marquardt algorithm and the general regression neural network produced a sum-of-squares error within 11.1{\%} of each other for the renewal dataset. The small number of inputs and poorly mapped input space on the second dataset meant that much larger errors were recorded on some of the test data. The potential for using neural networks for residual life prediction and the advantage of incorporating condition-based data into the model was nevertheless proven for both examples. {\textcopyright} 2008 Elsevier Ltd. All rights reserved.},
author = {Herzog, M. A. and Marwala, T. and Heyns, P. S.},
doi = {10.1016/j.ress.2008.05.008},
file = {:C$\backslash$:/Users/douwm/Google Drive/Meesters/Masters{\_}lib/2. C-AIM Publications/2009 Herzog Marwala Heyns Machine and component residual life estimation.pdf:pdf},
issn = {09518320},
journal = {Reliability Engineering and System Safety},
keywords = {Condition-monitoring data,Neural networks,Residual life},
number = {2},
pages = {479--489},
title = {{Machine and component residual life estimation through the application of neural networks}},
volume = {94},
year = {2009}
}
@article{Oberholster2009,
abstract = {The ability to monitor the vibration of blades online is of great importance to the structural health of turbomachinery. This paper focuses on the fixed reference frame or Eulerian implementation of laser Doppler vibrometry to perform this function. The way in which this measurement technique works is studied analytically and then a numerical simulation approach is proposed. Through experimental testing and finite element modeling, it is shown that this measurement approach is in principle viable for online blade condition monitoring when phase angles at reference frequencies are monitored, using non-harmonic Fourier analysis. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
